def merge_the_tools(string, k):
    c=0
    l=[]
    for i in string:
        c+=1
        if i not in l:
            l.append(i)
        if k==c:
            print(''.join(l))
            l=[]
            c=0
        
    
    
    

if __name__ == '__main__':
    string, k = input(), int(input())
    merge_the_tools(string, k)
